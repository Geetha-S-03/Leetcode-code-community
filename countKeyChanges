class Solution {
    public int countKeyChanges(String s) {
        String str=s.toLowerCase();
        int count=0;
        int len=str.length();
        for(int i=0;i<len-1;i++){
                if(str.charAt(i)!=str.charAt(i+1)){
                    count+=1;
                }
        }
        return count;
    }
}
